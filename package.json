{
  "name": "team-slots-api",
  "version": "0.1.0",
  "main": "src/server.ts",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16.13"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "calendar:init": "node --require dotenv-flow/config ./src/scripts/initialize.js",
    "start": "yarn ts .",
    "dev": "NODE_ENV=development nodemon -e ts -w ./src -x yarn ts .",
    "lint": "tsc && eslint src test",
    "format": "eslint app/**/*.ts --fix",
    "test": "yarn typecheck && yarn coverage",
    "coverage": "yarn jest --collectCoverage",
    "jest": "NODE_ENV=test yarn ts ./node_modules/.bin/jest --config .jest.json --runInBand",
    "typecheck": "tsc",
    "ts": "node --require dotenv-flow/config --require esbuild-register"
  },
  "dependencies": {
    "@sentry/node": "^6.17.1",
    "@sentry/tracing": "^6.16.1",
    "chalk": "4.1.2",
    "cors": "^2.8.5",
    "dotenv-flow": "3.2.0",
    "esbuild": "0.14.9",
    "esbuild-register": "3.3.1",
    "express": "4.17.1",
    "googleapis": "^95.0.0",
    "helmet": "^5.0.2",
    "luxon": "^2.3.0",
    "sentry": "^0.1.2",
    "uuid": "^8.3.2",
    "winston": "3.3.3",
    "winston-transport": "4.4.0",
    "zod": "3.11.6",
    "zod-express-middleware": "1.4.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv-flow": "3.2.0",
    "@types/express": "4.17.13",
    "@types/gapi.client.calendar": "^3.0.10",
    "@types/jest": "27.4.0",
    "@types/luxon": "^2.0.9",
    "@types/node": "16",
    "@types/supertest": "2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "25.3.2",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "3.3.0",
    "jest": "27.4.6",
    "nodemon": "2.0.7",
    "prettier": "2.5.1",
    "supertest": "6.1.6",
    "ts-jest": "27.1.2",
    "typescript": "^4.5.5"
  }
}
